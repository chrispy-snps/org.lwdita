<?xml version="1.0" encoding="UTF-8"?>
<topic xmlns:ditaarch="http://dita.oasis-open.org/architecture/2005/"
       ditaarch:DITAArchVersion="2.0"
       specializations="@props/audience @props/deliveryTarget @props/otherprops @props/platform @props/product" class="- topic/topic " id="header">
  <title class="- topic/title ">Header</title>
  <body class="- topic/body ">
    <p class="- topic/p " id="p" outputclass="draft" rev="1">Paragraph</p>
    <p class="- topic/p ">Inline <b class="+ topic/ph hi-d/b " outputclass="red">bold</b>.
    </p>
    <p class="- topic/p ">Inline <i class="+ topic/ph hi-d/i " outputclass="red">inline</i>.
    </p>
    <lq class="- topic/lq " audience="novice">
      <p class="- topic/p " outputclass="red">Quote</p>
    </lq>
    <ul class="- topic/ul " audience="novice">
      <li class="- topic/li ">
        <p class="- topic/p " id="ul">List item</p>
      </li>
      <li class="- topic/li ">
        <p class="- topic/p ">List item</p>
      </li>
    </ul>
    <ol class="- topic/ol " audience="novice">
      <li class="- topic/li ">
        <p class="- topic/p " id="ol">List item</p>
      </li>
      <li class="- topic/li ">
        <p class="- topic/p ">List item</p>
      </li>
    </ol>
    <dl class="- topic/dl " audience="novice">
      <dlentry class="- topic/dlentry ">
        <dt class="- topic/dt ">Definition Term</dt>
        <dd class="- topic/dd ">
          <p class="- topic/p " outputclass="red">definition item 1</p>
        </dd>
        <dd class="- topic/dd ">
          <p class="- topic/p ">definition item 2</p>
        </dd>
      </dlentry>
    </dl>
    <table class="- topic/table " audience="novice">
      <tgroup class="- topic/tgroup " cols="1">
        <colspec class="- topic/colspec " colname="col1"/>
        <thead class="- topic/thead ">
          <row class="- topic/row ">
            <entry class="- topic/entry ">head</entry>
          </row>
        </thead>
        <tbody class="- topic/tbody ">
          <row class="- topic/row ">
            <entry class="- topic/entry ">body</entry>
          </row>
        </tbody>
      </tgroup>
    </table>
    <codeblock audience="novice" class="+ topic/pre pr-d/codeblock " id="id" outputclass="lang" xml:space="preserve">code</codeblock>
    <fig class="- topic/fig ">
      <title class="- topic/title ">Title</title>
      <image class="- topic/image " href="test.jpg" id="carrot" outputclass="juice">
        <alt class="- topic/alt ">Alt</alt>
      </image>
    </fig>
    <image class="- topic/image " id="key-carrot" keyref="img_id" outputclass="juice" placement="break"/>
  </body>
</topic>
